<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoJavafx.TipoDePartidaController">
    <AnchorPane layoutY="16.0" prefHeight="426.0" prefWidth="643.0">
        <ImageView fitHeight="501.0" fitWidth="712.0" layoutX="-62.0" layoutY="-38.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@imagenes/tiposdepartida.jpeg"/>
        </ImageView>
        <Button fx:id="buttonCargarPartida" layoutX="150.0" layoutY="300.0" onAction="#handleCargarPartida"
                text="Cargar Partida"/>
        <Button fx:id="buttonNuevaPartida" layoutX="150.0" layoutY="200.0" onAction="#handleNuevaPartida"
                text="Nueva Partida"/>
    </AnchorPane>
</AnchorPane>
