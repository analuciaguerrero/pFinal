<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoJavafx.SeleccionarPartidaController">
    <children>
        <AnchorPane layoutY="16.0" prefHeight="426.0" prefWidth="643.0">
            <children>
                <ImageView fitHeight="501.0" fitWidth="712.0" layoutX="-62.0" layoutY="-38.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@imagenes/tiposdepartida.jpeg" />
                    </image>
                </ImageView>
                <Button fx:id="btnSeguirPartida" layoutX="164.0" layoutY="128.0" mnemonicParsing="false" onAction="#onBotonCargarPartidaClick" prefHeight="56.0" prefWidth="324.0" text="Seguir con partida">
                    <font>
                        <Font size="30.0" />
                    </font>
                </Button>
                <Button fx:id="btnNuevaPartida" layoutX="163.0" layoutY="238.0" mnemonicParsing="false" onAction="#onBottonNuevoClick" prefHeight="56.0" prefWidth="324.0" text="Nueva partida">
                    <font>
                        <Font size="30.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
